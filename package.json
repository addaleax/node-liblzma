{
  "name": "node-liblzma",
  "version": "0.4.1-rc2",
  "description": "Liblzma bindings for NodeJS",
  "main": "./lib/XzStream.js",
  "scripts": {
    "clean": "rm -rf node_modules build",
    "build": "node-gyp configure build",
    "debug": "node-gyp configure build --debug",
    "test": "./node_modules/.bin/plz -v clean test",
    "preinstall": "./node_modules/.bin/plz -v build",
    "prepublish": "./node_modules/.bin/plz -v build"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/oorabona/node-liblzma"
  },
  "keywords": [
    "node",
    "lzma",
    "lzma2",
    "xz",
    "compress",
    "uncompress",
    "decompress",
    "filters",
    "binding",
    "native"
  ],
  "author": "Olivier ORABONA",
  "licenses": [
    {
      "type": "LGPL3",
      "url": "https://github.com/oorabona/node-liblzma/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/oorabona/node-liblzma/issues"
  },
  "homepage": "https://github.com/oorabona/node-liblzma",
  "dependencies": {
    "plz": "^2.0.3"
  },
  "devDependencies": {
    "coffee-script": "latest",
    "expect.js": "^0.3.1",
    "mocha": "^1.21.4",
    "q": "^1.0.1"
  }
}
